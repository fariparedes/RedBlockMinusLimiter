{
  "manifest_description": {
    "message": "Improved Twitter-block-chain"
  },
  "popup_alert_non_twitter": {
    "message": "Please navigate to target's profile page(e.g. https://twitter.com/[userName]) and run it again."
  },
  "popup_extension_version_short": {
    "message": "Version: $version$",
    "placeholders": {
      "version": {
        "content": "$1"
      }
    }
  },
  "popup_extension_version_long": {
    "message": "Current Red Block version is $version$.",
    "placeholders": {
      "version": {
        "content": "$1"
      }
    }
  },
  "popup_chainblock_tooltip": {
    "message": "Block all followers"
  },
  "popup_chainblock_button": {
    "message": "Block-chain (Followers)"
  },
  "script_alert_before_unload": {
    "message": "If you leave to another page, Block-chain will stop. Are you sure to leave this page?"
  },
  "script_already_running": {
    "message": "Block-chain is already running."
  },
  "script_already_running_to_someone": {
    "message": "Block-chain is already running to $username$",
    "placeholders": {
      "username": {
        "content": "$1"
      }
    }
  },
  "script_confirm_stop": {
    "message": "Do you want to stop Block-chain?"
  },
  "script_ui_target_username": {
    "message": "$username$'s Follower",
    "placeholders": {
      "username": {
        "content": "@<span class=\"redblock-target-username\">$1</span>"
      }
    }
  },
  "script_ui_total_followers": {
    "message": "Total: $count$",
    "placeholders": {
      "count": {
        "content": "<span class=\"redblock-target-total-followers\">$1</span>"
      }
    }
  },
  "script_ui_blocked_followers": {
    "message": "Blocked: $count$",
    "placeholders": {
      "count": {
        "content": "<span class=\"redblock-blocked-user\">$1</span>"
      }
    }
  },
  "script_ui_progress": {
    "message": "Progress: $percentage$",
    "placeholders": {
      "percentage": {
        "content": "<span class=\"redblock-progress-percentage\">0</span>%"
      }
    }
  },
  "script_ui_already_blocked": {
    "message": "Already blocked: $count$",
    "placeholders": {
      "count": {
        "content": "<span class=\"redblock-already-blocked-user\">$1</span>"
      }
    }
  },
  "script_ui_skipped": {
    "message": "Skip: $count$",
    "placeholders": {
      "count": {
        "content": "<span class=\"redblock-skipped-user\">$1</span>"
      }
    }
  },
  "script_ui_failed": {
    "message": "Fail: $count$",
    "placeholders": {
      "count": {
        "content": "<span class=\"redblock-failed-user\">$1</span>"
      }
    }
  },
  "script_ui_rate_limited": {
    "message": "Rate-limited. Waiting..."
  },
  "script_ui_limit_reset_time": {
    "message": "Reset Time: $time$",
    "placeholders": {
      "time": {
        "content": "<span class=\"redblock-ratelimit-reset\">$1</span>"
      }
    }
  },
  "script_ui_close": {
    "message": "Close"
  },
  "script_ui_state_initial": {
    "message": "Preparing"
  },
  "script_ui_state_completed": {
    "message": "Completed"
  },
  "script_ui_state_running": {
    "message": "Runningâ€¦"
  },
  "script_ui_state_rate_limited": {
    "message": "Rate-limited"
  },
  "script_ui_state_stopped": {
    "message": "Stopped"
  },
  "script_ui_state_error": {
    "message": "Error!"
  },
  "script_ui_after_complete": {
    "message": "Block-chain Completed! (Blocked: $count$)",
    "placeholders": {
      "count": {
        "content": "$1"
      }
    }
  },
  "script_ui_after_stop": {
    "message": "Block-chain Stopped! (Blocked: $count$)",
    "placeholders": {
      "count": {
        "content": "$1"
      }
    }
  },
  "script_ui_alert_error": {
    "message": "Error occured in Block-chain!\nMessage: $error_message$",
    "placeholders": {
      "error_message": {
        "content": "$1"
      }
    }
  },
  "script_alert_unable_to_protected_user": {
    "message": "Can't execute Block-chain to protected account."
  },
  "script_alert_zero_follower": {
    "message": "There's no follower to that account"
  },
  "script_confirm_chain_block": {
    "message": "Are you sure to run Block-chain to $username$?",
    "placeholders": {
      "username": {
        "content": "@$1"
      }
    }
  },
  "script_alert_rate_limited": {
    "message": "Rate-limited. Please try again."
  }
}
